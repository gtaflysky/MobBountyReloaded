MobBountyReloaded is a new and updated version of MobBounty.

For support and help, go to: http://dev.bukkit.org/server-mods/mobbountyreloaded/

Commands:
    mobbounty:
        description: View rewards and fines.
        aliases: [mb]
        usage: /<command>
    mbgeneral:
        description: Modify the general settings.
        aliases: [mbg]
        usage: /<command> <property> <value>
    mbreward:
        description: Modify the default reward/fine of a mob.
        aliases: [mbr]
        usage: /<command> <mob> <amount>
    mbworldreward:
        description: Modify reward/fine of a mob in a world.
        aliases: [mbwr]
        usage: /<command> <world> <mob> <amount>
    mbenvmulti:
        description: Modify multiplier per environment.
        aliases: [mbem]
        usage: /<command> <normal|nether|end> <amount>
    mbtimemulti:
        description: Modify multiplier per time.
        aliases: [mbtm]
        usage: /<command> <day|sunset|night|sunrise> <amount>
    mbworldmulti:
        description: Modify multiplier per world.
        aliases: [mbwm]
        usage: /<command> <world> <amount>
    mbgroupmulti:
        description: Modify multiplier per group.
        aliases: [mbgm]
        usage: /<command> <group> <amount>
    mbsave:
        description: Save the config files.
        aliases: [mbs]
        usage: /<command>
    mbload:
        description: Load the config files.
        aliases: [mbl]
        usage: /<command>
    mbcheckdrop:
        description: Check drops for mobs in a world.
        aliases: [mbcd]
        usage: /<command>
    mbmodifydrop:
        description: Modify a mob's drop in the world sender is in.
        aliases: [mbmd]
        usage: /<command> [mob] <drop formula>
    mbworldmodifydrop:
        description: Modify a mob's drop in a world.
        aliases: [mbwmd]
        usage: /<command> [world] [mob] <drop formula>
    mbcheckexperience:
        description: Check exp for mobs in a world.
        aliases: [mbce]
        usage: /<command>
    mbexperience:
        description: Modify a mob's experience in the world sender is in.
        aliases: [mbe]
        usage: /<command> [mob] <experience>
    mbworldexperience:
        description: Modify a mob's experience in a world.
        aliases: [mbwe]
        usage: /<command> [world] [mob] <experience>

permissions:
    mbr.user.collect.normal:
